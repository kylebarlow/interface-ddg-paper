@article{tyka_efficient_2012,
	title = {Efficient sampling of protein conformational space using fast loop building and batch minimization on highly parallel computers},
	volume = {33},
	copyright = {Copyright {\textcopyright} 2012 Wiley Periodicals, Inc.},
	issn = {1096-987X},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/jcc.23069/abstract},
	doi = {10.1002/jcc.23069},
	abstract = {All-atom sampling is a critical and compute-intensive end stage to protein structural modeling. Because of the vast size and extreme ruggedness of conformational space, even close to the native structure, the high-resolution sampling problem is almost as difficult as predicting the rough fold of a protein. Here, we present a combination of new algorithms that considerably speed up the exploration of very rugged conformational landscapes and are capable of finding heretofore hidden low-energy states. The algorithm is based on a hierarchical workflow and can be parallelized on supercomputers with up to 128,000 compute cores with near perfect efficiency. Such scaling behavior is notable, as with Moore's law continuing only in the number of cores per chip, parallelizability is a critical property of new algorithms. Using the enhanced sampling power, we have uncovered previously invisible deficiencies in the Rosetta force field and created an extensive decoy training set for optimizing and testing force fields. {\textcopyright} 2012 Wiley Periodicals, Inc.},
	language = {en},
	number = {31},
	urldate = {2013-03-30},
	journal = {Journal of Computational Chemistry},
	author = {Tyka, Michael D. and Jung, Kenneth and Baker, David},
	year = {2012},
	keywords = {quals, conformational space search, energy landscape, loophash, parallel computing, Protein structure prediction, sampling, Optimization},
	pages = {2483--2491},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/QBBU4ZDM/Tyka et al. - 2012 - Efficient sampling of protein conformational space.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/J4EMANHV/abstract\;jsessionid=4C7C86E421B9003CB96BAE6102E860C1.html:text/html}
}


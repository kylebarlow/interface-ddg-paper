@article{claudio_prion_2011,
	title = {Prion hypothesis: the end of the controversy?},
	volume = {36},
	issn = {0968-0004},
	shorttitle = {Prion hypothesis},
	url = {http://www.sciencedirect.com/science/article/pii/S0968000410002100},
	doi = {10.1016/j.tibs.2010.11.001},
	abstract = {Forty-three years have passed since it was first proposed that a protein could be the sole component of the infectious agent responsible for the enigmatic prion diseases. Many discoveries have strongly supported the prion hypothesis, but only recently has this once heretical hypothesis been widely accepted by the scientific community. In the past 3 years, researchers have achieved the {\textquoteleft}Holy Grail{\textquoteright} demonstration that infectious material can be generated in vitro using completely defined components. These breakthroughs have proven that a misfolded protein is the active component of the infectious agent, and that propagation of the disease and its unique features depend on the self-replication of the infectious folding of the prion protein. In spite of these important discoveries, it remains unclear whether another molecule besides the misfolded prion protein might be an essential element of the infectious agent. Future research promises to reveal many more intriguing features about the rogue prions.},
	number = {3},
	urldate = {2012-02-06},
	journal = {Trends in Biochemical Sciences},
	author = {Claudio, Soto},
	month = mar,
	year = {2011},
	pages = {151--158},

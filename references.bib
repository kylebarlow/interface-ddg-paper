	journal = {Cell},
	author = {Qi, Lei~S. and Larson, Matthew~H. and Gilbert, Luke~A. and Doudna, Jennifer~A. and Weissman, Jonathan~S. and Arkin, Adam~P. and Lim, Wendell~A.},
	month = feb,
	year = {2013},
	keywords = {sgrnaproject},
	pages = {1173--1183},
	file = {ScienceDirect Full Text PDF:/home/kyleb/Zotero/storage/P72XV2PH/Qi et al. - 2013 - Repurposing CRISPR as an RNA-Guided Platform for S.pdf:application/pdf;ScienceDirect Snapshot:/home/kyleb/Zotero/storage/JKV5TTWR/S0092867413002110.html:text/html}
}

@article{david_contribution_2015,
	title = {The {Contribution} of {Missense} {Mutations} in {Core} and {Rim} {Residues} of {Protein}{\textendash}{Protein} {Interfaces} to {Human} {Disease}},
	volume = {427},
	issn = {0022-2836},
	url = {http://www.sciencedirect.com/science/article/pii/S0022283615003824},
	doi = {10.1016/j.jmb.2015.07.004},
	abstract = {Missense mutations at protein{\textendash}protein interaction sites, called interfaces, are important contributors to human disease. Interfaces are non-uniform surface areas characterized by two main regions, {\textquotedblleft}core{\textquotedblright} and {\textquotedblleft}rim{\textquotedblright}, which differ in terms of evolutionary conservation and physicochemical properties. Moreover, within interfaces, only a small subset of residues ({\textquotedblleft}hot spots{\textquotedblright}) is crucial for the binding free energy of the protein{\textendash}protein complex.

We performed a large-scale structural analysis of human single amino acid variations (SAVs) and demonstrated that disease-causing mutations are preferentially located within the interface core, as opposed to the rim (p \&lt; 0.01). In contrast, the interface rim is significantly enriched in polymorphisms, similar to the remaining non-interacting surface. Energetic hot spots tend to be enriched in disease-causing mutations compared to non-hot spots (p = 0.05), regardless of their occurrence in core or rim residues. For individual amino acids, the frequency of substitution into a polymorphism or disease-causing mutation differed to other amino acids and was related to its structural location, as was the type of physicochemical change introduced by the SAV.

In conclusion, this study demonstrated the different distribution and properties of disease-causing SAVs and polymorphisms within different structural regions and in relation to the energetic contribution of amino acid in protein{\textendash}protein interfaces, thus highlighting the importance of a structural system biology approach for predicting the effect of SAVs.},
	number = {17},
	urldate = {2015-08-26},
	journal = {Journal of Molecular Biology},
	author = {David, Alessia and Sternberg, Michael J. E.},
	month = aug,
	year = {2015},
	keywords = {core and rim interface, ddg, human disease, nsSNPs, protein{\textendash}protein interaction, SAVs},
	pages = {2886--2898},
	file = {ScienceDirect Full Text PDF:/home/kyleb/Zotero/storage/PUXRWQDH/David and Sternberg - 2015 - The Contribution of Missense Mutations in Core and.pdf:application/pdf;ScienceDirect Snapshot:/home/kyleb/Zotero/storage/KMDHGBCZ/S0022283615003824.html:text/html}
}


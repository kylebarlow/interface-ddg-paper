	month = jun,
	year = {2008},
	keywords = {CELLBIO, HUMDISEASE, MOLNEURO},
	pages = {681--693},
	file = {1-s2.0-S0896627308003280-main.pdf:/home/kyleb/Zotero/storage/PAKB7G9G/1-s2.0-S0896627308003280-main.pdf:application/pdf;ScienceDirect Full Text PDF:/home/kyleb/Zotero/storage/JRDVXR84/Jiang et al. - 2008 - ApoE Promotes the Proteolytic Degradation of A$\beta$.pdf:application/pdf;ScienceDirect Snapshot:/home/kyleb/Zotero/storage/W7I4PQXS/Jiang et al. - 2008 - ApoE Promotes the Proteolytic Degradation of A$\beta$.html:text/html;ScienceDirect Snapshot:/home/kyleb/Zotero/storage/HETUCECJ/Jiang et al. - 2008 - ApoE Promotes the Proteolytic Degradation of A$\beta$.html:text/html}
}

@article{cramer_apoe-directed_2012,

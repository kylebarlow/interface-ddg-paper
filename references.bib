	journal = {Nature},
	author = {Carette, Jan E. and Raaben, Matthijs and Wong, Anthony C. and Herbert, Andrew S. and Obernosterer, Gregor and Mulherkar, Nirupama and Kuehne, Ana I. and Kranzusch, Philip J. and Griffin, April M. and Ruthel, Gordon and Cin, Paola Dal and Dye, John M. and Whelan, Sean P. and Chandran, Kartik and Brummelkamp, Thijn R.},
	month = aug,
	year = {2011},
	keywords = {Molecular biology, Cell biology, Genetics and genomics, Virology},
	pages = {340--343},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/R69ATNTG/Carette et al. - 2011 - Ebola virus entry requires the cholesterol transpo.pdf:application/pdf}
}

@article{robertson_imaging_2010,

	keywords = {benchmarkpaper},
	pages = {2600--2607},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/UCFG3IP9/Moal and Fern{\'a}ndez-Recio - 2012 - SKEMPI a Structural Kinetic and Energetic databas.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/FSIKS29C/2600.html:text/html}

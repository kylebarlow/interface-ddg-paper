}

@article{wang_universal_2011,
	title = {A {Universal} {Molecular} {Clock} of {Protein} {Folds} and {Its} {Power} in {Tracing} the {Early} {History} of {Aerobic} {Metabolism} and {Planet} {Oxygenation}},
	volume = {28},
	issn = {0737-4038, 1537-1719},
	url = {http://mbe.oxfordjournals.org/content/28/1/567},
	doi = {10.1093/molbev/msq232},
	abstract = {The standard molecular clock describes a constant rate of molecular evolution and provides a powerful framework for evolutionary timescales. Here, we describe the existence and implications of a molecular clock of folds, a universal recurrence in the discovery of new structures in the world of proteins. Using a phylogenomic structural census in hundreds of proteomes, we build phylogenies and time lines of domains at fold and fold superfamily levels of structural complexity. These time lines correlate approximately linearly with geological timescales and were here used to date two crucial events in life history, planet oxygenation and organism diversification. We first dissected the structures and functions of enzymes in simulated metabolic networks. The placement of anaerobic and aerobic enzymes in the time line revealed that aerobic metabolism emerged about 2.9 billion years (giga-annum; Ga) ago and expanded during a period of about 400 My, reaching what is known as the Great Oxidation Event. During this period, enzymes recruited old and new folds for oxygen-mediated enzymatic activities. Remarkably, the first fold lost by a superkingdom disappeared in Archaea 2.6 Ga ago, within the span of oxygen rise, suggesting that oxygen also triggered diversification of life. The implications of a molecular clock of folds are many and important for the neutral theory of molecular evolution and for understanding the growth and diversity of the protein world. The clock also extends the standard concept that was specific to molecules and their timescales and turns it into a universal timescale-generating tool.},
	language = {en},
	number = {1},
	urldate = {2013-05-20},
	journal = {Molecular Biology and Evolution},
	author = {Wang, Minglei and Jiang, Ying-Ying and Kim, Kyung Mo and Qu, Ge and Ji, Hong-Fang and Mittenthal, Jay E. and Zhang, Hong-Yu and Caetano-Anoll{\'e}s, Gustavo},
	month = jan,
	year = {2011},
	pmid = {20805191},
	keywords = {enzyme, aerobic metabolism, evolution, function, structure},
	pages = {567--582},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/J5KH7WBG/Wang et al. - 2011 - A Universal Molecular Clock of Protein Folds and I.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/CJP7NHXD/567.html:text/html}
}

@article{ivankov_contact_2003,
	title = {Contact order revisited: {Influence} of protein size on the folding rate},
	volume = {12},

	pages = {a010454},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/P6BAEI78/Vendruscolo et al. - 2011 - Protein Solubility and Protein Homeostasis A Gene.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/4BGUE3UG/a010454.html:text/html}
}

@article{vendruscolo_proteome_2012,
	title = {Proteome folding and aggregation},
	volume = {22},
	issn = {0959-440X},
	url = {http://www.sciencedirect.com/science/article/pii/S0959440X12000073},
	doi = {10.1016/j.sbi.2012.01.005},
	abstract = {The description of protein folding at the proteome level requires further principles in addition to those that govern this phenomenon for individual molecules. An important aspect of the increased complexity of the folding process in the cellular environment is that proteins tend to be metastable against aggregation, as they are often expressed at levels at which they are poorly soluble. The maintenance of the solubility of the proteome requires the coordinated intervention of a range of quality control mechanisms, which include molecular chaperones, trafficking and degradation pathways, post-translational modifications and transcriptional and translational control. As these regulatory mechanisms should always be active to keep proteins in their soluble state, their impairment upon ageing or environmental stress can lead to the disruption of protein homeostasis resulting in uncontrolled widespread aggregation and disease.},
	number = {2},
	urldate = {2014-02-10},
	journal = {Current Opinion in Structural Biology},
	author = {Vendruscolo, Michele},
	month = apr,
	year = {2012},
	keywords = {ipqbjc2014},
	pages = {138--143},
	file = {ScienceDirect Full Text PDF:/home/kyleb/Zotero/storage/A56AR7QR/Vendruscolo - 2012 - Proteome folding and aggregation.pdf:application/pdf;ScienceDirect Snapshot:/home/kyleb/Zotero/storage/PKB6AIFU/S0959440X12000073.html:text/html}
}

@article{buck_role_2013,
	title = {On the {Role} of {Aggregation} {Prone} {Regions} in {Protein} {Evolution}, {Stability}, and {Enzymatic} {Catalysis}: {Insights} from {Diverse} {Analyses}},
	volume = {9},
	shorttitle = {On the {Role} of {Aggregation} {Prone} {Regions} in {Protein} {Evolution}, {Stability}, and {Enzymatic} {Catalysis}},
	url = {http://dx.doi.org/10.1371/journal.pcbi.1003291},
	doi = {10.1371/journal.pcbi.1003291},
	abstract = {Author SummaryBiotechnology requires the large-scale expression, yield, and storage of recombinant proteins. Each step in protein production has the potential to cause aggregation as proteins, not evolved to exist outside the cell, endure the various steps involved in commercial manufacturing processes. Mechanistic studies into protein aggregation have revealed that certain sequence regions contribute more to the aggregation propensity of a protein than other sequence regions do. Efforts to disrupt these regions have thus far indicated that rational sequence engineering is a useful technique to reduce the aggregation of biotechnologically relevant proteins. To improve our ability to rationally engineer proteins with enhanced expression, solubility, and shelf-life we conducted extensive analyses of aggregation prone regions (APRs) within protein sequences to characterize the various roles these regions play in proteins. Findings from this work indicate that protein sequences have evolved by minimizing their aggregation propensities. However, we also found that many APRs are conserved in protein families and are essential to maintain protein stability and function. Therefore, the contributions that APRs, targeted for disruption, make towards protein stability and function should be carefully evaluated when improving protein solubility via rational design.},
	number = {10},
	urldate = {2014-02-10},
	journal = {PLoS Comput Biol},
	author = {Buck, Patrick M. and Kumar, Sandeep and Singh, Satish K.},
	month = oct,
	year = {2013},
	keywords = {ipqbjc2014},
	pages = {e1003291},
	file = {PLoS Snapshot:/home/kyleb/Zotero/storage/FI88G8VW/infodoi10.1371journal.pcbi.html:text/html}
}


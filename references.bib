
@article{jahn_yin_2005,
	title = {The {Yin} and {Yang} of protein folding},
	volume = {272},
	issn = {1742-4658},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1742-4658.2005.05021.x/abstract},
	doi = {10.1111/j.1742-4658.2005.05021.x},
	abstract = {The study of protein aggregation saw a renaissance in the last decade, when it was discovered that aggregation is the cause of several human diseases, making this field of research one of the most exciting frontiers in science today. Building on knowledge about protein folding energy landscapes, determined using an array of biophysical methods, theory and simulation, new light is now being shed on some of the key questions in protein-misfolding diseases. This review will focus on the mechanisms of protein folding and amyloid fibril formation, concentrating on the role of partially folded states in these processes, the complexity of the free energy landscape, and the potentials for the development of future therapeutic strategies based on a full biophysical description of the combined folding and aggregation free-energy surface.},
	language = {en},
	number = {23},
	urldate = {2014-10-06},
	journal = {FEBS Journal},
	author = {Jahn, Thomas R. and Radford, Sheena E.},
	month = dec,
	year = {2005},
	keywords = {amyloid fibril formation, energy landscapes, intermediates, misfolding, protein folding},
	pages = {5962--5970},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/X9A3JQC3/Jahn and Radford - 2005 - The Yin and Yang of protein folding.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/2UGGSJC3/full.html:text/html}
}

@article{bottaro_subtle_2012,
	title = {Subtle {Monte} {Carlo} {Updates} in {Dense} {Molecular} {Systems}},
	volume = {8},
	issn = {1549-9618},
	url = {http://dx.doi.org/10.1021/ct200641m},
	doi = {10.1021/ct200641m},
	abstract = {Although Markov chain Monte Carlo (MC) simulation is a potentially powerful approach for exploring conformational space, it has been unable to compete with molecular dynamics (MD) in the analysis of high density structural states, such as the native state of globular proteins. Here, we introduce a kinetic algorithm, CRISP, that greatly enhances the sampling efficiency in all-atom MC simulations of dense systems. The algorithm is based on an exact analytical solution to the classic chain-closure problem, making it possible to express the interdependencies among degrees of freedom in the molecule as correlations in a multivariate Gaussian distribution. We demonstrate that our method reproduces structural variation in proteins with greater efficiency than current state-of-the-art Monte Carlo methods and has real-time simulation performance on par with molecular dynamics simulations. The presented results suggest our method as a valuable tool in the study of molecules in atomic detail, offering a potential alternative to molecular dynamics for probing long time-scale conformational transitions.},
	number = {2},
	urldate = {2014-07-14},
	journal = {Journal of Chemical Theory and Computation},
	author = {Bottaro, Sandro and Boomsma, Wouter and E. Johansson, Kristoffer and Andreetta, Christian and Hamelryck, Thomas and Ferkinghoff-Borg, Jesper},
	month = feb,
	year = {2012},
	keywords = {fromtanja},
	pages = {695--702},
	file = {ACS Full Text PDF w/ Links:/home/kyleb/Zotero/storage/SAH48U24/Bottaro et al. - 2012 - Subtle Monte Carlo Updates in Dense Molecular Syst.pdf:application/pdf;ACS Full Text Snapshot:/home/kyleb/Zotero/storage/2FF5B5E8/ct200641m.html:text/html}
}

@article{song_high-resolution_2013,
	title = {High-{Resolution} {Comparative} {Modeling} with {RosettaCM}},
	volume = {21},
	issn = {0969-2126},
	url = {http://www.cell.com/article/S0969212613002979/abstract},
	doi = {10.1016/j.str.2013.08.005},
	abstract = {We describe an improved method for comparative modeling, RosettaCM, which optimizes a physically realistic all-atom energy function over the conformational space defined by homologous structures. Given a set of sequence alignments, RosettaCM assembles topologies by recombining aligned segments in Cartesian space and building unaligned regions de novo in torsion space. The junctions between segments are regularized using a loop closure method combining fragment superposition with gradient-based minimization. The energies of the resulting models are optimized by all-atom refinement, and the most representative low-energy model is selected. The CASP10 experiment suggests that RosettaCM yields models with more accurate side-chain and backbone conformations than other methods when the sequence identity to the templates is greater than \~{}15\%.},
	language = {English},
	number = {10},
	urldate = {2014-07-14},
	journal = {Structure},
	author = {Song, Yifan and DiMaio, Frank and Wang, Ray Yu-Ruei and Kim, David and Miles, Chris and Brunette, T. J. and Thompson, James and Baker, David},
	month = aug,
	year = {2013},
	pmid = {24035711},
	keywords = {fromtanja},
	pages = {1735--1742},
	file = {Snapshot:/home/kyleb/Zotero/storage/3QJHN97A/S0969-2126(13)00297-9.html:text/html}
}

@article{damborsky_computational_2014,
	series = {Biocatalysis and biotransformation? {Bioinorganic} chemistry},

@article{ando_high-speed_2013,
	title = {High-{Speed} {AFM} and {Applications} to {Biomolecular} {Systems}},
	volume = {42},
	url = {http://dx.doi.org/10.1146/annurev-biophys-083012-130324},
	doi = {10.1146/annurev-biophys-083012-130324},
	abstract = {Directly observing individual protein molecules in action at high spatiotemporal resolution has long been a holy grail for biological science. This is because we long have had to infer how proteins function from the static snapshots of their structures and dynamic behavior of optical makers attached to the molecules. This limitation has recently been removed to a large extent by the materialization of high-speed atomic force microscopy (HS-AFM). HS-AFM allows us to directly visualize the structure dynamics and dynamic processes of biological molecules in physiological solutions, at subsecond to sub-100-ms temporal resolution, without disturbing their function. In fact, dynamically acting molecules such as myosin V walking on an actin filament and bacteriorhodopsin in response to light are successfully visualized. In this review, we first describe theoretical considerations for the highest possible imaging rate of this new microscope, and then highlight recent imaging studies. Finally, the current limitation and future challenges to explore are described.},
	number = {1},
	urldate = {2016-04-29},
	journal = {Annual Review of Biophysics},
	author = {Ando, Toshio and Uchihashi, Takayuki and Kodera, Noriyuki},
	year = {2013},
	pmid = {23541159},
	keywords = {atomic force microscopy, dynamics of biomolecules, high-resolution imaging, nanostructural dynamics, proteins in action},
	pages = {393--414}
}

@article{proctor_nonnative_2016,
	title = {Nonnative {SOD}1 trimer is toxic to motor neurons in a model of amyotrophic lateral sclerosis},
	volume = {113},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org.ucsf.idm.oclc.org/content/113/3/614},
	doi = {10.1073/pnas.1516725113},
	abstract = {Since the linking of mutations in the Cu,Zn superoxide dismutase gene (sod1) to amyotrophic lateral sclerosis (ALS) in 1993, researchers have sought the connection between SOD1 and motor neuron death. Disease-linked mutations tend to destabilize the native dimeric structure of SOD1, and plaques containing misfolded and aggregated SOD1 have been found in the motor neurons of patients with ALS. Despite advances in understanding of ALS disease progression and SOD1 folding and stability, cytotoxic species and mechanisms remain unknown, greatly impeding the search for and design of therapeutic interventions. Here, we definitively link cytotoxicity associated with SOD1 aggregation in ALS to a nonnative trimeric SOD1 species. We develop methodology for the incorporation of low-resolution experimental data into simulations toward the structural modeling of metastable, multidomain aggregation intermediates. We apply this methodology to derive the structure of a SOD1 trimer, which we validate in vitro and in hybridized motor neurons. We show that SOD1 mutants designed to promote trimerization increase cell death. Further, we demonstrate that the cytotoxicity of the designed mutants correlates with trimer stability, providing a direct link between the presence of misfolded oligomers and neuron death. Identification of cytotoxic species is the first and critical step in elucidating the molecular etiology of ALS, and the ability to manipulate formation of these species will provide an avenue for the development of future therapeutic strategies.},
	language = {en},
	number = {3},
	urldate = {2016-04-29},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Proctor, Elizabeth A. and Fee, Lanette and Tao, Yazhong and Redler, Rachel L. and Fay, James M. and Zhang, Yuliang and Lv, Zhengjian and Mercer, Ian P. and Deshmukh, Mohanish and Lyubchenko, Yuri L. and Dokholyan, Nikolay V.},
	month = jan,
	year = {2016},
	pmid = {26719414},
	keywords = {Protein aggregation, protein misfolding, ALS, neurodegeneration, structural modeling},
	pages = {614--619},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/6J6UTNTV/Proctor et al. - 2016 - Nonnative SOD1 trimer is toxic to motor neurons in.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/JBERPPKS/614.html:text/html}
}


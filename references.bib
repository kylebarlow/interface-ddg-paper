	keywords = {loop modeling, loop closure, polynomial systems, protein kinematics, resultants},
	pages = {176--189},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/7GM7ZSVI/Coutsias et al. - 2006 - Resultants and loop closure.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/G4WSQKT2/abstract.html:text/html}
}

@article{smith_backrub-like_2008,
	title = {Backrub-{Like} {Backbone} {Simulation} {Recapitulates} {Natural} {Protein} {Conformational} {Variability} and {Improves} {Mutant} {Side}-{Chain} {Prediction}},

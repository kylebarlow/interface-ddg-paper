	author = {Dukart, Juergen and Kherif, Ferath and Mueller, Karsten and Adaszewski, Stanislaw and Schroeter, Matthias L. and Frackowiak, Richard S. J. and Draganski, Bogdan and {for the Alzheimer's Disease Neuroimaging Initiative}},

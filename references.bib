	file = {Full Text PDF:/home/kyleb/Zotero/storage/T6MDQNGN/Lukk et al. - 2012 - Homology models guide discovery of diverse enzyme .pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/45IT5XNZ/4122.html:text/html}
}


	keywords = {protein design, Algorithm engineering, Branch-and-bound, Computational Intelligence, Lagrangian relaxation, Numerical and Computational Physics, Operations Research/Decision Theory, side-chain placement, Optimization},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/RDGJHFEZ/Canzar et al. - 2011 - An exact algorithm for side-chain placement in pro.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/BV6DXWRX/10.html:text/html}
}


	month = jul,
	year = {2012},
	pmid = {22383579},
	keywords = {sgrnaproject},
	pages = {5775--5786},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/9FJEV855/Qi et al. - 2012 - Engineering naturally occurring trans-acting non-c.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/WWF2V4SC/5775.html:text/html}

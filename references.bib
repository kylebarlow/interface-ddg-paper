	year = {2011},
	pages = {e19230},
	file = {PLoS Full Text PDF:/home/kyleb/Zotero/storage/RFBKA69G/Richter et al. - 2011 - De Novo Enzyme Design Using Rosetta3.pdf:application/pdf}
}


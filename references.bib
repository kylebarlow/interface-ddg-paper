	keywords = {Amyotrophic lateral sclerosis, Embryos, Hyperexpression techniques, Messenger RNA, Motor neurons, Motor proteins, Phenotypes, Zebrafish},
	pages = {e1002214},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/S7CC7FSN/Kabashi et al. - 2011 - FUS and TARDBP but Not SOD1 Interact in Genetic Mo.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/AN26KAGG/article.html:text/html}

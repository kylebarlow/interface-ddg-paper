	file = {Full Text PDF:/home/kyleb/Zotero/storage/6QWEWNM9/Moult et al. - 2014 - Critical assessment of methods of protein structur.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/SBA8ZBDD/full.html:text/html}
}


	urldate = {2012-04-04},
	journal = {Molecular Cell},
	author = {Chawla, Ajay and Boisvert, William A. and Lee, Chih-Hao and Laffitte, Bryan A. and Barak, Yaacov and Joseph, Sean B. and Liao, Debbie and Nagy, Laszlo and Edwards, Peter A. and Curtiss, Linda K. and Evans, Ronald M. and Tontonoz, Peter},
	month = jan,
	year = {2001},
	pages = {161--171},
	file = {ScienceDirect Full Text PDF:/home/kyleb/Zotero/storage/FSA2SMJN/Chawla et al. - 2001 - A PPAR$\gamma$-LXR-ABCA1 Pathway in Macrophages Is Involv.pdf:application/pdf;ScienceDirect Snapshot:/home/kyleb/Zotero/storage/UFD4NXE5/Chawla et al. - 2001 - A PPAR$\gamma$-LXR-ABCA1 Pathway in Macrophages Is Involv.html:text/html}

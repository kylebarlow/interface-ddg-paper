	keywords = {computational protein design, protein engineering, benchmarkpaper, energy functions, estimating protein stability},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/JRIRE4ME/Potapov et al. - 2009 - Assessing computational methods for predicting pro.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/SRMK8653/553.html:text/html}

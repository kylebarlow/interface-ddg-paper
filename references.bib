}

@article{vendruscolo_proteome_2012,
	title = {Proteome folding and aggregation},
	volume = {22},
	issn = {0959-440X},
	url = {http://www.sciencedirect.com/science/article/pii/S0959440X12000073},
	doi = {10.1016/j.sbi.2012.01.005},
	abstract = {The description of protein folding at the proteome level requires further principles in addition to those that govern this phenomenon for individual molecules. An important aspect of the increased complexity of the folding process in the cellular environment is that proteins tend to be metastable against aggregation, as they are often expressed at levels at which they are poorly soluble. The maintenance of the solubility of the proteome requires the coordinated intervention of a range of quality control mechanisms, which include molecular chaperones, trafficking and degradation pathways, post-translational modifications and transcriptional and translational control. As these regulatory mechanisms should always be active to keep proteins in their soluble state, their impairment upon ageing or environmental stress can lead to the disruption of protein homeostasis resulting in uncontrolled widespread aggregation and disease.},
	pages = {138--143},
	number = {2},
	journaltitle = {Current Opinion in Structural Biology},
	shortjournal = {Current Opinion in Structural Biology},
	author = {Vendruscolo, Michele},
	urldate = {2014-02-10},

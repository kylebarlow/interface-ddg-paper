@article{block_relation_1966,
	title = {The {Relation} of {Temperature} to the {Chirp}-{Rate} of {Male} {Snowy} {Tree} {Crickets}, {Oecanthus} fultoni ({Orthoptera}: {Gryllidae})},
	volume = {59},
	shorttitle = {The {Relation} of {Temperature} to the {Chirp}-{Rate} of {Male} {Snowy} {Tree} {Crickets}, {Oecanthus} fultoni ({Orthoptera}},
	abstract = {During 35 observation periods in August and September 1955 at University Park, Penn., 758 chirp-rate determinations were made on different individual or simultaneously chirping wild males of Oecanthus fultoni T. Walker. Records were obtained at 19 of the 29 half-degree intervals between 9.5{\textdegree} and 23.5{\textdegree}C. The mean chirp-rate per minute varied from 46 at 9.5{\textdegree} to 144 at 23.5{\textdegree}. The individual ranges from the means at the several temperatures varied from 3\% to 32\%. An exponential relationship between chirp-rate and temperature was demonstrated, but no definite relationship between chirp-rate and relative humidity was discovered. The appreciable variation in chirp-rates at given temperatures was attributed to uncontrolled environmental variables and to species and subspecific variability.},
	number = {1},
	journal = {Annals of the Entomological Society of America},
	author = {BLOCK, BARTLEY C.},
	year = {1966},
	keywords = {Crickets},
	pages = {56--59}
}

@article{doherty_temperature_1985,
	title = {Temperature {Coupling} and {\textquoteleft}{Trade}-{Off}{\textquoteright} {Phenomena} in the {Acoustic} {Communication} {System} of the {Cricket}, {Gryllus} {Bimaculatus} {De} {Geer} ({Gryllidae})},

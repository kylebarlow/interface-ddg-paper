@article{shi_human_2012,
	title = {A {Human} {Stem} {Cell} {Model} of {Early} {Alzheimer}{\textquoteright}s {Disease} {Pathology} in {Down} {Syndrome}},
	volume = {4},
	issn = {1946-6234, 1946-6242},
	url = {http://stm.sciencemag.org/content/4/124/124ra29},
	doi = {10.1126/scitranslmed.3003771},
	abstract = {AbstractBack to Top
Human cellular models of Alzheimer{\textquoteright}s disease (AD) pathogenesis would enable the investigation of candidate pathogenic mechanisms in AD and the testing and developing of new therapeutic strategies. We report the development of AD pathologies in cortical neurons generated from human induced pluripotent stem (iPS) cells derived from patients with Down syndrome. Adults with Down syndrome (caused by trisomy of chromosome 21) develop early-onset AD, probably due to increased expression of a gene on chromosome 21 that encodes the amyloid precursor protein (APP). We found that cortical neurons generated from iPS cells and embryonic stem cells from Down syndrome patients developed AD pathologies over months in culture, rather than years in vivo. These cortical neurons processed the transmembrane APP protein, resulting in secretion of the pathogenic peptide fragment amyloid-$\beta$42 (A$\beta$42), which formed insoluble intracellular and extracellular amyloid aggregates. Production of A$\beta$ peptides was blocked by a $\gamma$-secretase inhibitor. Finally, hyperphosphorylated tau protein, a pathological hallmark of AD, was found to be localized to cell bodies and dendrites in iPS cell{\textendash}derived cortical neurons from Down syndrome patients, recapitulating later stages of the AD pathogenic process.},
	language = {en},
	number = {124},
	urldate = {2012-04-03},
	journal = {Science Translational Medicine},
	author = {Shi, Yichen and Kirwan, Peter and Smith, James and MacLean, Glenn and Orkin, Stuart H and Livesey, Frederick J},
	month = mar,
	year = {2012},
	pages = {124ra29--124ra29},
	file = {Full Text PDF:/home/kyleb/Zotero/storage/X2GBJKKM/Shi et al. - 2012 - A Human Stem Cell Model of Early Alzheimer{\textquoteright}s Disea.pdf:application/pdf;Snapshot:/home/kyleb/Zotero/storage/A7CX8BXU/Shi et al. - 2012 - A Human Stem Cell Model of Early Alzheimer{\textquoteright}s Disea.html:text/html}

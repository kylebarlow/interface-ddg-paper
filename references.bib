	pages = {515--529},
	file = {ScienceDirect Full Text PDF:/home/kyleb/Zotero/storage/MVHWMGAC/Majumdar et al. - 2012 - Critical Role of Amyloid-like Oligomers of Drosoph.pdf:application/pdf;ScienceDirect Snapshot:/home/kyleb/Zotero/storage/CRWXU2VJ/Majumdar et al. - 2012 - Critical Role of Amyloid-like Oligomers of Drosoph.html:text/html}

@article{bhabha_keep_2015,
	title = {Keep on Moving: Discovering and Perturbing the Conformational Dynamics of Enzymes},
	volume = {48},
	issn = {0001-4842},
	url = {http://dx.doi.org/10.1021/ar5003158},
	doi = {10.1021/ar5003158},
	shorttitle = {Keep on Moving},
	abstract = {{ConspectusBecause} living organisms are in constant motion, the word ?dynamics? can hold many meanings to biologists. Here we focus specifically on the conformational changes that occur in proteins and how studying these protein dynamics may provide insights into enzymatic catalysis.Advances in integrating techniques such as X-ray crystallography, nuclear magnetic resonance, and electron cryomicroscopy (cryo {EM}) allow us to model the dominant structures and exchange rates for many proteins and protein complexes. For proteins amenable to atomic resolution techniques, the major questions shift from simply describing the motions to discovering their role in function. Concurrently, there is an increasing need for using perturbations to test predictive models of dynamics?function relationships. Examples are the catalytic cycles of dihydrofolate reductase ({DHFR}) and cyclophilin A ({CypA}).In {DHFR}, mutations that alter the ability of the active site to sample productive higher energy states on the millisecond time scale reduce the rate of hydride transfer significantly. Recently identified rescue mutations restore function, but the mechanism by which they do so remains unclear. The exact role of any changes in the dynamics remains an open question. For {CypA}, a network of side chains that exchange between two conformations is critical for catalysis. Mutations that lock the network in one state also reduce catalytic activity.A further understanding of enzyme dynamics of well-studied enzymes such as dihydrofolate reductase and cyclophilin A will lead to improvement in ability to modulate the functions of computationally designed enzymes and large macromolecular machines. In designed enzymes, directed evolution experiments increase catalytic rates. Detailed X-ray studies suggest that these mutations likely limit the conformational space explored by residues in the active site. For proteins where atomic resolution information is currently inaccessible, other techniques such as cryo-{EM} and high-resolution single molecule microscopy continue to advance. Understanding the conformational dynamics of larger systems such as protein machines will likely become more accessible and provide new opportunities to rationally modulate protein function.},
	pages = {423--430},
	number = {2},
	journaltitle = {Accounts of Chemical Research},
	shortjournal = {Acc. Chem. Res.},
	author = {Bhabha, Gira and Biel, Justin T. and Fraser, James S.},
	urldate = {2015-06-08},
	date = {2015-02-17},
	file = {ACS Full Text PDF w/ Links:/home/kyleb/Zotero/storage/ERNP4I5C/Bhabha et al. - 2015 - Keep on Moving Discovering and Perturbing the Con.pdf:application/pdf;ACS Full Text Snapshot:/home/kyleb/Zotero/storage/G588TTWZ/ar5003158.html:text/html}
}

@article{dourado_multiscale_2014,
